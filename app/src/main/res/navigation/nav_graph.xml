<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/oneFragment">

    <fragment
        android:id="@+id/twoFragment"
        android:name="com.example.mytestingproject.fragment.TwoFragment"
        android:label="two_fragment"
        tools:layout="@layout/two_fragment" >
        <argument
            android:name="title"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/oneFragment"
        android:name="com.example.mytestingproject.fragment.OneFragment"
        android:label="one_fragment_layout"
        tools:layout="@layout/one_fragment_layout" >
        <action
            android:id="@+id/action_oneFragment_to_twoFragment"
            app:destination="@id/twoFragment" />
        <action
            android:id="@+id/action_oneFragment_to_threeFragment"
            app:destination="@id/threeFragment" />
    </fragment>
    <fragment
        android:id="@+id/threeFragment"
        android:name="com.example.mytestingproject.fragment.ThreeFragment"
        android:label="three_fragment_layout"
        tools:layout="@layout/three_fragment_layout" >
        <argument
            android:name="title"
            app:argType="string" />
    </fragment>
</navigation>